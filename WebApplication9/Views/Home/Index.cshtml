@model  List<Endemik_Bitkiler>



<div style="text-align: center;">
    <h1>Endemik Bitkiler</h1>
</div>


<div>
    @foreach (var x in Model)
    {
        <p style="font-size: 16px; color: crimson; font-weight: bold;">@x.baslik</p>

        <img src="@x.fotourl" alt="" width="900" ; height="450" />
        <br />
        <br />
        <p style="font-size: 16px; color: black; font-weight: bold;">@x.açıklama</p>
        
        <br />



    }

</div>






